  </Association>
  <Association Name="Routes_Stops">
    <End Type="CoachCompanyModel.Route" Role="Route" Multiplicity="*" />
    <End Type="CoachCompanyModel.Stop" Role="Stop" Multiplicity="*" />
  </Association>
